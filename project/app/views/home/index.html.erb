
<div class="col-md-6">
	<div class="col-md-12">
		<%= form_tag("", method: "post") do %>
		<%= label_tag(:q, "Search for:") %>
	<br/><br/>
	Fichier json: <%= file_field_tag 'json', class: "form-control" %>
	</div>
	<br/>
	<div class="col-md-4">
		Sensibilité: <br/><%= number_field_tag("name", nil, in: 0...100, step: 0.01, class: "form-control")  %>
		<br/>
		<%= submit_tag 'Lancer la vérification', class: 'btn' %>
	</div>
<% end %>
</div>